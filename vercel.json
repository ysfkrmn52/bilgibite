{
  "version": 2,
  "buildCommand": "npx esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ]
}
